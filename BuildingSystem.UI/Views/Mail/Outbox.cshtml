@model List<MessageDto>
    @{
    ViewData["Title"] = "Outbox";
    Layout = "~/Views/Shared/_OccupantLayout.cshtml";
    }


    <table id="myDataTable" class="table table-hover table-striped">
        <thead class="thead-light">
            <tr>
                <th scope="col">Gönderici</th>
                <th scope="col">Mesaj Başlığı</th>
                <th scope="col">Mesaj İçeriği</th>



            </tr>
        </thead>
        <tbody>
            @foreach (var menu in Model)
        {
            <tr>
                <td>@menu.UserName</td>
                <td>@Html.Raw(@menu.MessageContent)</td>
                <td>@Html.Raw(@menu.Body)</td>


            </tr>
        }
        </tbody>
    </table>
